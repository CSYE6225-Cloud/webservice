spring.jpa.hibernate.ddl-auto=none
spring.datasource.url=jdbc:mysql://${MYSQL_HOST:localhost}:3306/{databasename}?sslMode=VERIFY_IDENTITY&trustCertificateKeyStoreUrl=file:/usr/local/etc/rds.keystore&trustCertificateKeyStorePassword=
spring.datasource.username=
spring.datasource.password=
spring.datasource.driver-class-name=com.mysql.jdbc.Driver
# spring.jpa.show-sql=true

# table name strategy
spring.jpa.hibernate.naming.physical-strategy=org.hibernate.boot.model.naming.PhysicalNamingStrategyStandardImpl

logging.level.org.springframework=DEBUG
#output to a temp_folder/file
logging.file.name=./application.log
## Logging pattern for the console
#logging.pattern.console= %d{yyyy-MM-dd HH:mm:ss} - %msg%n
# Logging pattern for file
logging.pattern.file= %d{yyyy-MM-dd HH:mm:ss} [%thread] %-5level %logger{36} - %msg%

server.port=8080

# AWS
aws.region=
aws.aws.bucket-name=
aws.dynamodb-table-name=
aws.topic-arn=
aws.dynamodb-expired-time=
aws.dynamodb-email-tracker-table-name=
